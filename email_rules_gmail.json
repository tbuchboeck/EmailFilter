{
  "rules": [
    {
      "name": "Gaming » Steam",
      "folder": "INBOX/Gaming/Steam",
      "conditions": {
        "from_contains": ["steampowered.com"]
      }
    },
    {
      "name": "Gaming » Nintendo",
      "folder": "INBOX/Gaming/Nintendo",
